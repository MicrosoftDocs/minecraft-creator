{
  "id": "cmd_ride",
  "title": "ride",
  "description": "Makes entities ride other entities, stops entities from riding, makes rides evict their riders, or summons rides or riders.",
  "fields": [
    {
      "id": "riders",
      "title": "riders",
      "type": "string",
      "isOptional": false,
      "description": "Specifies riders using player name or [target selector](https://learn.microsoft.com/minecraft/creator/documents/commandsintroduction#target-selectors)s.",
      "subFormId": "type_selection",
      "commandType": "SELECTION"
    },
    {
      "id": "mode",
      "title": "mode",
      "type": "string",
      "isOptional": false,
      "commandType": "RIDEMODESTART"
    },
    {
      "id": "ride",
      "title": "ride",
      "type": "string",
      "isOptional": false,
      "description": "Specifies the entity to ride on. A player name or [target selector](https://learn.microsoft.com/minecraft/creator/documents/commandsintroduction#target-selectors).",
      "subFormId": "type_selection",
      "commandType": "SELECTION"
    },
    {
      "id": "teleportrules",
      "title": "teleportRules",
      "type": "string",
      "isOptional": true,
      "description": "Either `teleport_ride` or `teleport_rider` (default).",
      "commandType": "TELEPORTRULES"
    },
    {
      "id": "howtofill",
      "title": "howToFill",
      "type": "string",
      "isOptional": true,
      "description": "One of `if_group_fits` to check whether riders can ride and only makes them ride if possible. Or `until_full` which makes them ride until rides is full. Default: `until_full`.",
      "commandType": "FILLTYPE"
    },
    {
      "id": "rides",
      "title": "rides",
      "type": "string",
      "isOptional": false,
      "description": "Specifies the ridden entities. Must be a player name or entity selector.",
      "subFormId": "type_selection",
      "commandType": "SELECTION"
    },
    {
      "id": "entitytype",
      "title": "entityType",
      "type": "string",
      "isOptional": false,
      "description": "Specifies entities to summon. Must be the name`ID`of something that can be summoned.",
      "subFormId": "type_entitytype",
      "commandType": "ENTITYTYPE"
    },
    {
      "id": "spawnevent",
      "title": "spawnEvent",
      "type": "string",
      "isOptional": true,
      "description": "Specifies the in-game event to call the summoned entities. Must be a spawn event in behavior pack.",
      "subFormId": "type_id",
      "commandType": "ID"
    },
    {
      "id": "nametag",
      "title": "nameTag",
      "type": "string",
      "isOptional": true,
      "description": "Specifies the name of summoned entities.",
      "subFormId": "type_id",
      "commandType": "ID"
    },
    {
      "id": "riderules",
      "title": "rideRules",
      "type": "string",
      "isOptional": true,
      "description": "Ride rules",
      "commandType": "RIDERULES"
    }
  ],
  "overloads": [
    {
      "id": "1",
      "title": "Start ride",
      "description": "Start `ride` with `riders`, something to `ride`, and optional `teleport rules` and `fill type`.",
