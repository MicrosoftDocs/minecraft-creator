      "subFormId": "type_id",
      "commandType": "ID"
    },
    {
      "id": "preload",
      "title": "preload",
      "type": "string",
      "isOptional": true,
      "description": "A `Boolean` that queries the preload value. Default: `false`.",
      "subFormId": "type_boolean",
      "commandType": "BOOLEAN"
    },
    {
      "id": "circle",
      "title": "circle",
      "type": "string",
      "isOptional": false,
      "commandType": "ADDTICKINGAREATYPE"
    },
    {
      "id": "center",
      "title": "center",
      "type": "string",
      "isOptional": false,
      "description": "A `vector` [x y z] that specifies the center of a defined loaded area.",
      "subFormId": "type_position",
      "commandType": "POSITION"
    },
    {
      "id": "radius",
      "title": "radius",
      "type": "string",
      "isOptional": false,
      "description": "An `integer` that specifies the area as the number of chunks loaded in. Between 0 and 4.",
      "subFormId": "type_int",
      "commandType": "INT"
    },
    {
      "id": "position",
      "title": "position",
      "type": "string",
      "isOptional": false,
      "description": "A `vector` [x y z] of a point within the ticking area.",
      "subFormId": "type_position",
      "commandType": "POSITION"
    },
    {
      "id": "all-dimensions",
      "title": "all-dimensions",
      "type": "string",
      "isOptional": true,
      "description": "Specify `all-dimensions` to determine whether ticking areas in other dimensions are listed.",
