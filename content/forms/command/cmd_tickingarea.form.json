{
  "id": "cmd_tickingarea",
  "title": "tickingarea",
  "description": "Add, remove, or list ticking areas. See more information in the [Ticking Area documentation](https://learn.microsoft.com/minecraft/creator/documents/tickingareacommand).",
  "fields": [
    {
      "id": "mode",
      "title": "mode",
      "type": "string",
      "isOptional": false,
      "commandType": "TICKINGAREAMODEADD"
    },
    {
      "id": "from",
      "title": "from",
      "type": "string",
      "isOptional": false,
      "description": "A `vector` [x y z] that along with `to` of two opposite corners of a rectangular region to test for.",
      "subFormId": "type_position",
      "commandType": "POSITION"
    },
    {
      "id": "to",
      "title": "to",
      "type": "string",
      "isOptional": false,
      "description": "A `vector` [x y z] that along with `from` of two opposite corners of a rectangular region to test for.",
      "subFormId": "type_position",
      "commandType": "POSITION"
    },
    {
      "id": "name",
