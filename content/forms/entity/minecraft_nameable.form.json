{
  "fields": [],
  "generated_doNotEdit": {
    "id": "minecraft:nameable",
    "title": "Nameable",
    "description": "Allows this entity to be named (e.g. using a name tag).",
    "fields": [
      {
        "id": "allow_name_tag_renaming",
        "title": "Allow Name Tag Renaming",
        "description": "If true, this entity can be renamed with name tags",
        "defaultValue": true,
        "dataType": 1
      },
      {
        "id": "always_show",
        "title": "Always Show",
        "description": "If true, the name will always be shown",
        "defaultValue": false,
        "dataType": 1,
        "samples": {
          "/vanilla/behavior_pack/entities/player.json": [
            {
              "path": "/minecraft:entity/components/minecraft:nameable/",
              "content": true
            }
          ]
        }
      },
      {
        "id": "default_trigger",
        "title": "Default Trigger",
        "description": "Trigger to run when the entity gets named",
        "dataType": 2,
        "samples": {
          "/vanilla/behavior_pack/entities/vindicator.json": [
            {
              "path": "/minecraft:entity/components/minecraft:nameable/",
              "content": {
                "event": "minecraft:stop_johnny",
                "target": "self"
              }
            }
          ]
        }
      },
      {
        "id": "name_actions",
        "title": "Name Actions",
        "description": "Describes the special names for this entity and the events to call when the entity acquires those names",
        "dataType": 15,
        "subForm": {
          "id": "",
          "description": "Describes the special names for this entity and the events to call when the entity acquires those names",
          "fields": [
            {
              "id": "name_filter",
              "title": "Name Filter",
              "description": "List of special names that will cause the events defined in 'on_named' to fire",
              "dataType": 2
            },
            {
              "id": "on_named",
              "title": "Named",
              "description": "Event to be called when this entity acquires the name specified in 'name_filter'",
              "dataType": 22
            }
          ]
        },
        "samples": {
          "/vanilla/behavior_pack/entities/vindicator.json": [
            {
              "path": "/minecraft:entity/components/minecraft:nameable/",
              "content": [
                {
                  "name_filter": "Johnny",
                  "on_named": {
                    "event": "minecraft:start_johnny",
                    "target": "self"
                  }
                }
              ]
            }
          ]
        }
      }
    ],
